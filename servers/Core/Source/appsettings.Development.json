{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "Host=coredb;Port=5432;Database=coredb;Username=postgres;Password=postgres;Include Error Detail=True"
  },
  "Parser": {
    "Url": "http://parser:8000"
  },
  "TelegramBot": {
    "Url": "http://bot:8000"
  },
  "MainAdmin": {
    "Name": "admin",
    "Password": "admin"
  },
  "ReverseProxy": {
    "Routes": {
      "parser-route": {
        "ClusterId": "parser-cluster",
        "Match": {
          "Path": "/parser/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "parser-cluster": {
        "Destinations": {
          "destination": {
            "Address": "http://parser:8000"
          }
        }
      }
    }
  }
}