{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "Host=coredb;Port=5432;Database=coredb;Username=postgres;Password=postgres;Include Error Detail=True",
    "BlobStorage": "UseDevelopmentStorage=true;DevelopmentStorageProxyUri=http://blob-storage",
    "Parser": "http://parser:8000",
    "TelegramBot": "http://bot:8000"
  },
  "FileStorage": {
    "ContainerName": "documents"
  },
  "MainAdmin": {
    "Name": "admin",
    "Password": "admin"
  },
  "ReverseProxy": {
    "Routes": {
      "upload-route": {
        "ClusterId": "parser-cluster",
        "Metadata": {
          "Description": "Загружает файл со студентами"
        },
        "Match": {
          "Methods": [
            "Post",
            "Get"
          ],
          "Path": "/parser/upload/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "upload/{**catch-all}"
          }
        ]
      },
      "parser-route": {
        "ClusterId": "parser-cluster",
        "Metadata": {
          "Description": "Перенаправляет в парсер"
        },
        "Match": {
          "Methods": [
            "Get"
          ],
          "Path": "/parser/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "parser-cluster": {
        "Destinations": {
          "destination": {
            "Address": "http://parser:8000"
          }
        }
      }
    }
  }
}